    "url": "https://github.com/fiskeben/sidensolhverv.dk/issues"
  },
  "homepage": "https://github.com/fiskeben/sidensolhverv.dk",
  "dependencies": {
    "axios": "^0.17.1",
    "google-maps": "^3.3.0",
    "google-maps-api": "^2.0.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-google-maps": "^9.4.5",
    "react-share": "^1.19.1",
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
